pragma solidity >= 0.5.0 < 0.7.0;
contract realestate{
 
struct certificates{

	// State variables
	uint certificate_id;
	uint plot_register_id;
	string certificatepath;
	
	string date;
	

}

 
certificates []m;



function addcertificate(uint certificate_id,uint plot_register_id,string memory certificatepath,string memory date) public{
	certificates memory m1=certificates(certificate_id,plot_register_id,certificatepath,date);
	m.push(m1);
}




}




